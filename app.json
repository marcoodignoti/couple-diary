{
  "expo": {
    "name": "Couple Diary",
    "slug": "couple-diary",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "couplediary",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FAF9F6"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourname.couplediary"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FAF9F6"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.yourname.couplediary"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "name": "Couple Diary",
      "shortName": "Couple Diary",
      "description": "Un diario intimo per coppie a distanza con rivelazione settimanale",
      "lang": "it",
      "backgroundColor": "#FAF9F6",
      "themeColor": "#E8B4B8",
      "display": "standalone",
      "orientation": "portrait",
      "startUrl": "/"
    },
    "plugins": [
      "expo-router",
      "@react-native-community/datetimepicker",
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "0331078e-0a7a-4909-8cfa-7bd7d9712341"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/0331078e-0a7a-4909-8cfa-7bd7d9712341"
    }
  }
}